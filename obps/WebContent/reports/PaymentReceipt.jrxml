<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="PaymentReceipt" language="groovy" pageWidth="595" pageHeight="842" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="transactioncode" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT OFFICENAME1 || CHR(10) || CASE WHEN OFFICENAME2 IS NOT NULL THEN OFFICENAME2 ELSE '' END ||  CHR(10) || CASE WHEN OFFICENAME3 IS NOT NULL THEN OFFICENAME3 ELSE '' END AS OFFICE,
FULLNAME, FEETYPEDESCRIPTION, T.AMOUNT, PAYMENTMODEDESCRIPTION , T.TRANSACTIONCODE, A.APPLICATIONCODE
FROM
TRANSACTIONS T, applicationstransactionmap ATM, APPLICATIONS A, USERLOGINS U, MASTERS.MODULES M, MASTERS.PAYMENTMODES PM, MASTERS.FEETYPES FT, MASTERS.FEEMASTER FM
, MASTERS.OFFICES O
WHERE T.TRANSACTIONCODE = ATM.TRANSACTIONCODE
AND A.APPLICATIONCODE = ATM.APPLICATIONCODE
AND A.USERCODE = U.USERCODE
AND M.MODULECODE = A.MODULECODE
AND PM.PAYMENTMODECODE = T.PAYMENTMODECODE
AND FM.FEECODE = T.FEECODE
AND FT.FEETYPECODE = FM.FEETYPECODE
AND O.OFFICECODE  = A.OFFICECODE
AND PAYMENTSTATUS = 'S'
AND t.transactioncode=$P{transactioncode}]]>
	</queryString>
	<field name="office" class="java.lang.String"/>
	<field name="fullname" class="java.lang.String"/>
	<field name="feetypedescription" class="java.lang.String"/>
	<field name="amount" class="java.lang.Integer"/>
	<field name="paymentmodedescription" class="java.lang.String"/>
	<field name="transactioncode" class="java.lang.Long"/>
	<field name="applicationcode" class="java.lang.String"/>
